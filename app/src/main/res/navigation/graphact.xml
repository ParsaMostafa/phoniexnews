<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/graphact"
    app:startDestination="@id/fragmentlog">

    <fragment
        android:id="@+id/fragmentlog"
        android:name="com.example.phoenixnews.fragments.Fragmentlog"
        android:label="Fragmentlog" >
        <action
            android:id="@+id/action_fragmentlog_to_newsActivity"
            app:destination="@id/newsActivity" />
    </fragment>
    <activity
        android:id="@+id/newsActivity"
        android:name="com.example.phoenixnews.NewsActivity"
        android:label="NewsActivity" />
</navigation>