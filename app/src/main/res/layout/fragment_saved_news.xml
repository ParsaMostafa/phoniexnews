<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="424dp"
        android:layout_height="match_parent">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewsavednews"
            android:layout_width="411dp"
            android:layout_height="568dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="13dp"
            app:layout_constraintBottom_toTopOf="@+id/buttonBackup"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.029" />

        <Button
            android:id="@+id/buttonBackup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="107dp"
            android:gravity="center"
            android:text="StartingBackup"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/recyclerViewsavednews"
            app:layout_constraintStart_toStartOf="@+id/recyclerViewsavednews"
            app:layout_constraintTop_toBottomOf="@+id/recyclerViewsavednews" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>